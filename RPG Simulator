-- IN TESTING

local victim = workspace.Mobs:FindFirstChild("Grass Monster")
print(victim)
local char = game.Players.LocalPlayer.Character
local hrp = char.HumanoidRootPart
hrp.Anchored = false
repeat
    hrp.RotVelocity = Vector3.new(0,0,0)
    hrp.Velocity = Vector3.new(0,0,0)
char:MoveTo(victim.HumanoidRootPart.Position-Vector3.new(0,8,0))
hrp.CFrame = CFrame.lookAt(hrp.Position,victim.HumanoidRootPart.Position)
--char:MoveTo(victim.HumanoidRootPart.Position-Vector3.new(0,-3,0))
wait(0)
until
victim.Humanoid.Health <= 0
